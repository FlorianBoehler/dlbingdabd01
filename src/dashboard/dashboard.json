    "dataSources": {
        "ds_px4LR3Ee": {
            "type": "ds.search",
            "options": {
                "query": "sourcetype=_json \n| timechart span=1min avg(temperature) as \"Temperatur\" by machine useother=false\n| eval \"Critical\"=85\n| eval \"Warning\"=75"
            },
            "name": "All_Measurepoints"
        },
        "ds_nb3uZ39b": {
            "type": "ds.search",
            "options": {
                "query": "sourcetype=_json machine=\"ROTOR_M01\"\n| stats latest(temperature) as temp by machine \n| eval color=case(\n    temp < 75, \"green\",\n    temp >= 75 AND temp <= 85, \"yellow\",\n    temp > 85, \"red\"\n) \n| fields machine, temp, color\n"
            },
            "name": "Live_M1"
        },
        "ds_6ROPx3yv_ds_nb3uZ39b": {
            "type": "ds.search",
            "options": {
                "query": "sourcetype=_json machine=\"ROTOR_M02\"\n| stats latest(temperature) as temp by machine \n| eval color=case(\n    temp < 75, \"green\",\n    temp >= 75 AND temp <= 85, \"yellow\",\n    temp > 85, \"red\"\n) \n| fields machine, temp, color\n"
            },
            "name": "Live_M2"
        },
        "ds_YpSY6KqJ_ds_nb3uZ39b": {
            "type": "ds.search",
            "options": {
                "query": "sourcetype=_json machine=\"ROTOR_M03\"\n| stats latest(temperature) as temp by machine \n| eval color=case(\n    temp < 75, \"green\",\n    temp >= 75 AND temp <= 85, \"yellow\",\n    temp > 85, \"red\"\n) \n| fields machine, temp, color\n"
            },
            "name": "Live_M3"
        },
        "ds_QdGUStjd_ds_YpSY6KqJ_ds_nb3uZ39b": {
            "type": "ds.search",
            "options": {
                "query": "sourcetype=_json machine=\"ROTOR_M04\"\n| stats latest(temperature) as temp by machine \n| eval color=case(\n    temp < 75, \"green\",\n    temp >= 75 AND temp <= 85, \"yellow\",\n    temp > 85, \"red\"\n) \n| fields machine, temp, color\n"
            },
            "name": "Live_M4"
        },
        "ds_dXcDLaxQ_ds_QdGUStjd_ds_YpSY6KqJ_ds_nb3uZ39b": {
            "type": "ds.search",
            "options": {
                "query": "sourcetype=_json machine=\"ROTOR_M05\"\n| stats latest(temperature) as temp by machine \n| eval color=case(\n    temp < 75, \"green\",\n    temp >= 75 AND temp <= 85, \"yellow\",\n    temp > 85, \"red\"\n) \n| fields machine, temp, color\n"
            },
            "name": "Live_M5"
        },
        "ds_jRjUEicz": {
            "type": "ds.search",
            "options": {
                "query": "sourcetype=_json \n| where temperature > 75\n| bin _time span=30s\n| table _time, machine, temperature \n| eval _time=strftime(_time, \"%Y-%m-%d %H:%M:%S\")\n| rename _time as \"Zeitpunkt\", machine as \"Messpunkt-ID\", temperature as \"Maximaltemperatur °C\""
            },
            "name": "Critical_Values"
        }
    },
